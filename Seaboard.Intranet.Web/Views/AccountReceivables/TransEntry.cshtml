        function transferir() {
            if (confirm("Esta seguro que desea transferir las transacciones marcadas ?")) {
                var isAllValid = true;

                if ($('#batchId').val().trim() === '') {
                    isAllValid = false;
                    toastr.error('El campo Id. de lote es Requerido', 'Campo requerido');
                }

                if ($('#description').val().trim() === '') {
                    isAllValid = false;
                    toastr.error('El campo descripción es Requerido', 'Campo requerido');
                }

                if ($('#resolución').val().trim() === '') {
                    isAllValid = false;
                    toastr.error('El campo numero de resolución es Requerido', 'Campo requerido');
                }

                if (isAllValid) {
                    $('#transTable > tbody > tr').each(function() {
                        if (($(this).find('td:eq(0) input').prop("checked")) === true) {
                            $.ajax({
                                url: '@Url.Action("PostBatchTransEntryDetail", "AccountReceivables")?batchId=' +
                                    $('#batchId').val().trim() +
                                    '&customerId=' +
                                    $(this).find('td:eq(1)').html().trim() +
                                    '&fechaDocumento=01/' +
                                    $('#month').datepicker({ dateFormat: 'ddMMyyyy' }).val() +
                                    '&resolución=' +
                                    $('#resolución').val().trim(),
                                type: "POST",
                                data: "",
                                dataType: "JSON",
                                cache: false,
                                async: false,
                                contentType: "application/json",
                                success: function(data) {
                                    if (data !== "OK") {
                                        alert(data);
                                        $('#buttonPanel').children('.ibox-content').removeClass('sk-loading');
                                        $('#headerPanel').children('.ibox-content').removeClass('sk-loading');
                                    }
                                }
                            });
                        }
                    });

                    $('#modal-Question').modal('show');
                }
            }
        }

        function irGeneracion() {
            window.location.href = "@Url.Action("TransCreateIndex", "AccountReceivables")";
        }

        function irNeteo() {
            window.location.href = "@Url.Action("NetIndex", "AccountReceivables")";
        }

        function cancelar() {
            window.location.href = "@Url.Action("TransIndex", "AccountReceivables")";
        }

        function cerrarLote() {
            if ($('#batchId').val().trim().length > 0) {
                if (confirm("Esta seguro que desea cerrar el lote actual ?")) {
                    $('#transTable > tbody > tr').each(function() {
                        $.ajax({
                            url: '@Url.Action("CloseBatchTransEntry", "AccountReceivables")?batchId=' +
                                $('#batchId').val().trim() +
                                '&customerId=' +
                                $(this).find('td:eq(1)').html().trim(),
                            type: "POST",
                            data: "",
                            dataType: "JSON",
                            cache: false,
                            async: false,
                            contentType: "application/json",
                            success: function(data) {
                                if (data !== "OK") {
                                    alert(data);
                                }
                            }
                        });
                    });
                    window.location.href = "@Url.Action("TransIndex", "AccountReceivables")";
                }
            }
        }

        function eliminar() {
            if ($('#batchId').val().trim().length > 0) {
                if (confirm("Esta seguro que desea eliminar el lote actual ?")) {
                    $.ajax({
                        url: '@Url.Action("DeleteBatchTransEntry", "AccountReceivables")?batchId=' +
                            $('#batchId').val().trim(),
                        type: "POST",
                        data: "",
                        dataType: "JSON",
                        cache: false,
                        async: false,
                        contentType: "application/json",
                        success: function(data) {
                            if (data.status !== "OK") {
                                alert(data.status);
                            } else {
                                window.location.href = "@Url.Action("TransIndex", "AccountReceivables")";
                            }
                        }
                    });
                }
            }
        }

        function imprimir() {
            url = '@Url.Content("~/PDF/Reportes/")AccountReceivablesMemTrans.pdf';
            toastr.info('Estamos procesando su solicitud', 'Por favor espere');
            $.ajax({
                url: '@Url.Action("PrintTransEntry", "AccountReceivables")?date=01/' +
                    $('#month').datepicker({ dateFormat: 'yyyy/MM/dd' }).val(),
                type: "POST",
                data: "",
                cache: false,
                dataType: "JSON",
                contentType: "application/json",
                success: function(d) {
                    if (d.status === "OK") {
                        window.open(url);
                    } else {
                        alert(d.status);
                    }
                }
            });
        }
